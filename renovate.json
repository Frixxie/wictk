{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "groupName": "Cargo",
      "matchDatasources": ["cargo"],
      "matchManagers": ["cargo"],
      "labels": ["cargo"]
    },
    {
      "groupName": "Docker",
      "matchDatasources": ["dockerfile"],
      "matchManagers": ["dockerfile"],
      "labels": ["docker"]
    },
    {
      "groupName": "github",
      "matchDatasources": ["github-actions"],
      "matchManagers": ["github-actions"],
      "labels": ["github"]
    }
  ]
}
